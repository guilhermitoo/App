<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fatecriopreto.edu.br.appriori" >
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CadastrarActivity"
            android:label="@string/title_activity_cadastrar" >
        </activity>
        <activity
            android:name=".EsqueciActivity"
            android:label="@string/title_activity_esqueci" >
        </activity>
        <activity
            android:name=".HomeActivity"
            android:label="@string/title_activity_home" >
        </activity>
        <activity
            android:name=".ChamadoActivity"
            android:label="@string/title_activity_chamado" >
        </activity>
        <activity
            android:name=".AcompanharActivity"
            android:label="@string/title_activity_acompanhar" >
        </activity>
    </application>

</manifest>
